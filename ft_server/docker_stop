echo "Containers that are running :"
docker ps
if [ ! "$(docker ps -q -f name=nginx)" ]; then
    if [ "$(docker ps -aq -f status=exited -f name=nginx)" ]; then
   	docker stop nginix
	docker rm nginx
	echo "There is no Nginx container anymore :"
	docker ps
fi
	echo "There is no Nginx container."
fi
docker system prune
